    "flex-direction": {
      "property": "flex-direction",
      "class": "flex",
      "responsive": true,
      "description": "Dirección del eje principal (horizontal o vertical)",
      "values": {
        "row": "row",
        "column": "column"
      }
    },
    "flex-wrap": {
      "property": "flex-wrap",
      "class": "flex",
      "responsive": true,
