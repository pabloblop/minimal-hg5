{
  "name": "minimal-hg5",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:sass": "sass src/scss:src/css --quiet-deps --silence-deprecation=import",
    "build:css": "npm run build:sass && postcss src/css/docs.css -o _site/css/docs.css && postcss src/css/dutti.css -o _site/css/dutti.css",
    "watch:sass": "sass src/scss:src/css --watch --quiet-deps --silence-deprecation=import",
    "watch:css": "postcss src/css/docs.css -o _site/css/docs.css --watch & postcss src/css/dutti.css -o _site/css/dutti.css --watch",
    "dev": "npm run build:css && concurrently \"npm run watch:sass\" \"npm run watch:css\" \"eleventy --serve\"",
    "build": "npm run build:css && eleventy",
    "clean": "rm -rf _site"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "holygrail2": "^1.3.55",
    "holygrail5": "^1.0.12"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "sass": "^1.69.5"
  }
}
